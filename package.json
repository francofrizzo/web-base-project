{
  "name": "web-base",
  "version": "1.0.0",
  "description": "Basic framework to develop Express + Vue + Typescript app",
  "main": "dist/App.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:server": "tsc",
    "build:browser": "webpack",
    "build": "run-p build:browser build:server --print-label",
    "watch:build:server": "tsc --watch",
    "watch:build:browser": "webpack --watch",
    "watch:build": "run-p watch:build:server watch:build:browser",
    "watch:app": "nodemon './dist/server/App.js' --watch './dist/server'",
    "app": "node './dist/server/App.js'",
    "start": "npm-run-all clean build watch:app --print-label"
  },
  "author": "Franco Frizzo",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.14.0",
    "css-loader": "^3.5.3",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.22.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^8.0.2",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.17.0",
    "webpack-cli": "^3.3.11"
  },
  "keywords": []
}
